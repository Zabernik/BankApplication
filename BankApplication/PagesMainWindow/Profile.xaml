        <Label Grid.Column="2" Content="-" HorizontalAlignment="Center" Height="33" Margin="0,327,0,0" VerticalAlignment="Top" Width="16" FontSize="15" FontWeight="Bold"/>
        <TextBlock x:Name="TextBlockName" Grid.Column="3" HorizontalAlignment="Left" Height="33" Margin="46,101,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="197" Text="" Foreground="#FF393A3E" FontSize="15" FontWeight="Bold"/>
